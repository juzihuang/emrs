<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="true" useStatementNamespaces="true" maxRequests="512"
		maxSessions="128" maxTransactions="128" />
	<sqlMap resource="conf/ibatis/InpatientArea.xml" />
    <sqlMap resource="conf/ibatis/AdmEmployee.xml" />
	<sqlMap resource="conf/ibatis/AdmDepartment.xml" />
	<sqlMap resource="conf/ibatis/CommonNumAndNurse.xml" />
	<sqlMap resource="conf/ibatis/CareRecord.xml"/>
	<sqlMap resource="conf/ibatis/TemperatureRecord.xml"/>
	<sqlMap resource="conf/ibatis/BloodSugarRecord.xml"/>
	<sqlMap resource="conf/ibatis/BloodTransfusionRecord.xml"/>
	<sqlMap resource="conf/ibatis/IntensiveCareRecord.xml"/>
</sqlMapConfig>